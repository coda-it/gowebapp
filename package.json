{
  "name": "gowebapp",
  "version": "1.0.2",
  "description": "A customizable web platform",
  "main": "index.js",
  "scripts": {
    "build:dev": "webpack --mode=development --config webpack.dev.config.js",
    "build:prod": "webpack --mode=production --config webpack.prod.config.js",
    "lint": "eslint --ext .js,.jsx .",
    "lint:fix": "eslint --fix .",
    "test": "jest --config ./config/jest.config.js --no-watchman",
    "test:update": "jest --config ./config/jest.config.js --no-watchman --updateSnapshot",
    "csslint": "stylelint \"./client/**/*.scss\" --config .stylelintrc",
    "csslint:fix": "stylelint \"./client/**/*.scss\" --fix --config .stylelintrc",
    "flow": "flow",
    "prettify": "prettier --write \"./client/**/*.{js,jsx}\" \"./cypress/**/*.{js,jsx}\"",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/coda-it/gowebapp.git"
  },
  "author": "Oskar Szura",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/coda-it/gowebapp/issues"
  },
  "homepage": "https://github.com/coda-it/gowebapp#readme",
  "dependencies": {
    "graphen": "^1.8.5",
    "lodash": "^4.17.19",
    "react": "^16.5.2",
    "react-dom": "^16.5.2",
    "react-redux": "^5.0.7",
    "react-router": "^4.3.1",
    "redux": "^3.7.2",
    "redux-saga": "^0.16.0"
  },
  "devDependencies": {
    "@babel/core": "^7.11.1",
    "@babel/plugin-proposal-class-properties": "^7.10.4",
    "@babel/plugin-proposal-optional-chaining": "^7.11.0",
    "@babel/preset-env": "^7.11.0",
    "@babel/preset-flow": "^7.10.4",
    "@babel/preset-react": "^7.10.4",
    "autoprefixer": "^6.5.0",
    "babel-cli": "^6.26.0",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.1.0",
    "css-loader": "^3.6.0",
    "cypress": "^3.8.3",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "enzyme-to-json": "^3.5.0",
    "eslint": "^5.14.1",
    "eslint-config-codait": "^1.0.0",
    "file-loader": "^2.0.0",
    "flow-bin": "^0.94.0",
    "jest": "^24.1.0",
    "mini-css-extract-plugin": "^0.4.4",
    "node-sass": "^4.14.1",
    "postcss-loader": "^3.0.0",
    "prettier": "^1.19.1",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.13.1",
    "stylelint": "^9.6.0",
    "stylelint-config-standard": "^18.2.0",
    "uglifyjs-webpack-plugin": "^2.0.1",
    "webpack": "^4.44.1",
    "webpack-cli": "^3.3.12"
  }
}
